<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - diploma</title>
    <link rel="icon" type="image" href="~/img/icon1.svg">
    <!-- Open Graph метатеги -->
    <meta property="og:title" content="Hey! This is my diploma work) Welcome!" />
    <meta property="og:description" content="In my diploma i was working on Graphs theory end TPS problem) (on reaal map), welcome and lets see" />
    <meta property="og:image" content="https://diplmy.fun/img/cover.jpg" />
    <meta property="og:url" content="https://diplmy.fun" />
    <meta property="og:type" content="website" />
    <meta property="og:image:width" content="1280">
    <meta property="og:image:height" content="720">
    @*  <!-- Twitter Card метатеги-->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content="Custom Map Type Control" />
    <meta name="twitter:description" content="An example of a Google Maps with a custom map type control button." />
    <meta name="twitter:image" content="https://example.com/path/to/your/image.jpg" /> *@
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100..900&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="stylesheet" href="~/css/login.css">

</head>
<body>
    <div class="wrapper">
        <div class="sidebar">
            <a asp-area="" asp-controller="Home" asp-action="Index" class="menu-btn">
                <span><img src="~/img/icon1.svg" alt="Icon"></span>
            </a>
            <div class="sidebar-buttons">
                <a asp-area="" asp-controller="Home" asp-action="Login" class="idk-btn">
                    <span><img src="~/img/icon2.svg" alt="Icon"></span>
                </a>
                <a asp-area="" asp-controller="Home" asp-action="Routes" class="routes-btn">
                    <span><img src="~/img/icon3.svg" alt="Icon"></span>
                </a>
            </div>
        </div>





        @RenderBody()

    </div>








    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/mapsManager.js" asp-append-version="true"></script>
    <script src="https://kit.fontawesome.com/e1bc65fca0.js" crossorigin="anonymous"></script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAdNqLjq5sBbA9n2dAJNFcLsKdhw50zerw&libraries=places&callback=initMap"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
